%ul.ui-tabs-nav.ui-helper-reset.ui-helper-clearfix.ui-widget-header.ui-corner-all
  %li.ui-state-default.ui-corner-top.ui-tabs-selected.ui-state-active= link_to I18n.t('page', :count => 2).capitalize, '#content'
  %li.ui-state-default.ui-corner-top= link_to I18n.t('block', :count => 2).capitalize, admin_static_content_blocks_path
  %li.ui-state-default.ui-corner-top= link_to I18n.t('navigation').capitalize, admin_menus_path
#blocks-header.backgrounds.big-header.settings.ui-tabs.ui-widget.ui-widget-content.ui-corner-all
  %ul.ui-tabs-nav.ui-helper-reset.ui-helper-clearfix.ui-widget-header.ui-corner-all
    %li.ui-state-default.ui-corner-top.ui-tabs-selected.ui-state-active= link_to 'Principal', '#general', :class => 'selected'
    %li.ui-state-default.ui-corner-top= link_to 'blocks', '#blocks'
    %li.ui-state-default.ui-corner-top= link_to 'SEO', '#seo'
  .interact-links
    = fg_submit_tag('save')
    = t('or')
    = link_to t('cancel').capitalize, admin_pages_path, :class => 'back-link'
  .clear
