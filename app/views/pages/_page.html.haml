- page.blocks.each do |block|
  - case block
  - when Carousel
    .carousel-list
      %ul{ :id => "carousel_#{block.id}" }
        - block.items.each do |item|
          %li
            = link_to image_tag(item.picture.public_filename(:normal), :alt => item.picture.name ), item.url
            %span= item.title
  - else
    %div{ :id => "block_#{block.id}" }= raw block.content
- page.page_cols.each do |column|
  %div{ :id => "column#{column.id}" }= raw column.content

=# statistics_collector_tag(page)
